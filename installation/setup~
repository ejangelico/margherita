# UNIX user name - postgresql needs to know that
username: xxx
# name of the database to be used
database: xcap
#
# name of user directory, either full path starting with / or relative to here
# full path example: /home/xxx/Incom/xcap/characterization/user/
# relative example: ././user
user directory: ./../user/
# name of system directory, same way as for user directory
system directory: ./../C3PO/
# name of logging directory, same way as for user directory
logging directory: ./../logs/

